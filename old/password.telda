include lib/std.telda, lib/strcmp.telda

correct:
dat "Access Granted!", 0
wrong:
dat "Access Denied!", 0
prompt:
dat "Enter password:", 0
buf:
dat "000000000000000000000000000000000"
password:
data "PASS", 0

start:
pushw prompt
call Print

pushw buf
call Getl
pushw password
call Strcmp
cmp $a, 0

jez success

pushw wrong
call Print

add $sp, 8

jump start


success:
pushw correct
call Print
halt