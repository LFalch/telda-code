hello:
    dat "Hello World"
hello_end:
    dat "\n"

start:
    load $ba, hello
loop:
    # Load the character at the current index and print it
    load [$ba]
    int2
    # If it's the end, we're done now
    cmp $ba, hello_end
    jez end
    # Increment the character index
    inc $ba

    jmp loop

end:
    halt