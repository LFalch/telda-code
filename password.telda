include lib/std.telda, lib/strcmp.telda

correct:
dat "Access Granted!", 0
wrong:
dat "Access Denied!", 0
prompt:
dat "Enter password:", 0
buf:
dat "000000000000000000000000000000000"
password:
data "PASS", 0

start:
    push prompt
    call Print

    push buf
    call Getl
    push buf
    push password
    call Strcmp
    cmp $ab, 0

    jez success

    push wrong
    call Print

    jump start

success:
    push correct
    call Print
    halt