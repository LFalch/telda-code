include lib/io.telda, lib/convert.telda

start:
    # i = 0
    load $ac, ~0
loop:
    # i
    push 0, $ac

    # inttos(i, num_str); print(num_str);
    push 0, $ac
    push num_str
    call IntTos
    push num_str
    call Print

    # i++
    pop $ac
    inc $ac
    # if(i == 100) halt();
    cmp $ac, ~100
    jez Halt
    jmp loop
num_str: