buf:
    .string 000000000000000000000000000000000
correct:
    .string Access Granted!\0
wrong:
    .string Access Denied!\0
prompt:
    .string Enter password:\0
password:
    .string PASS\0

.ref _strcmp
.ref _print
.ref _readLine

.global _start
_start:
    add a, 0, prompt
    call _print

    add a, 0, buf
    call _readLine
    add a, 0, buf
    add x, 0, password
    call _strcmp
    sub 0, al, 0

    jez success

    add a, 0, wrong
    call _print

    jump _start

success:
    add a, 0, correct
    call _print
    halt
