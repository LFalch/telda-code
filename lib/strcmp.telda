# [$, *s1, *s2, ...]
# Returns $ab, 0 if equal
# sets flags according to $ab too
Strcmp:
    load $sr, [$sp+2]
    load $ds, [$sp+4]
    sdp
loop_Strcmp:
    sld $ab
    load $bb, [$ds]

    # compare and return if not equal
    sub $ab, $bb
    jnz return

    # check if any is zero
    load $ab, $bb
    jez return
    # manually increment $ds, since we can only string load from $sr
    inc $ds
    jump loop_Strcmp
return:
    ret 4
