Halt:
start:
    halt

Pc:
    load [$sp]
    ret
Pc_b:
    load $ba, [$sp]
    ret

Write:
    load $ba, [$sp+2]
loop_Write:
    load [$ba]
    cmp '\0'
    jez Return
    int2
    inc $ba
    jmp loop_Write

Print:
    load $ba, [$sp+2]
    call loop_Write
    load '\n'
    int2
Return:
    ret

Getl:
    load $ba, [$sp+2]

loop_Getl:
    int1
    cmp $ab, '\n'
    jez end_Getl
    str $ab, $ba
    inc $ba

    jump loop_Getl
end_Getl:
    load $ab, 0
    str $ab, $ba
    ret
